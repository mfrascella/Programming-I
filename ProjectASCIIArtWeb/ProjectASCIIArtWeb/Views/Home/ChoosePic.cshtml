@{
    ViewBag.Title = "ChoosePic";
}

<style>
    body {
        background-color: aliceblue;
    }

    h2 {
        text-align: center;
    }

    form {
        font-size: 16px;
    }
</style>

<h2>Aladdin Characters</h2>


@for (int i = 0; i < ViewBag.timesPrinted; i++)
{
    <pre>@ViewBag.Pictures</pre><br /><br />
}

<form action="Index">
    <input type="submit" value="Try again" />
</form>
